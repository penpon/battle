{
  "id": "starter-05",
  "difficulty": "Starter",
  "type": "hole",
  "timeLimitSec": 90,
  "prepare": {
    "image": "ubuntu:22.04",
    "files": null
  },
  "statement": "カレントディレクトリに testdir という空のディレクトリを作成してください。",
  "validators": {
    "regex": {
      "allow": [
        "^\\s*mkdir(\\s+-p)?\\s+testdir\\s*$"
      ],
      "deny": []
    },
    "effect": {
      "script": "validators/starter-05.js"
    }
  },
  "allowlistBins": [
    "bash",
    "sh",
    "mkdir",
    "ls",
    "echo",
    "pwd",
    "date"
  ],
  "allowlistPreset": "starter_wide"
}
