  "validators": {
    "effect": {
      "script": "validators/pro-04.js"
    }
  },
  "allowlistBins": [
    "bash",
    "sh",
    "grep",
    "cat",
    "sort",
    "uniq",
    "tee",
    "awk",
    "sed",
    "echo",
    "pwd",
    "ls"
  ],
  "allowlistPreset": "pro_task"
}
